{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "interface-name": false,
        "whitespace": [
            true,
            "check-module"
        ],
        "max-classes-per-file": false,
        "member-access": false,
        "ordered-imports": [
            true
        ],
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "semicolon": [
            true,
            "always",
            "ignore-bound-class-methods"
        ],
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "no-console": false,
        "indent": [
            true,
            "spaces",
            2
        ],
        "no-empty-interface": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-bitwise": false,
        "arrow-parens": false,
        "trailing-comma": false,
        "no-unused-variable": [
            true
        ],
        "prefer-const": [
            true,
            {
                "destructuring": "any",
                "ignoreReadBeforeAssign": false
            }
        ]
    },
    "rulesDirectory": []
}